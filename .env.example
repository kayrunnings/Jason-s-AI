# Miro Integration Configuration
# Copy this file to .env and fill in your credentials

# Miro API Access Token (get from https://miro.com/app/settings/user-profile/apps)
MIRO_ACCESS_TOKEN=your_access_token_here

# Default Miro Board ID (from board URL: https://miro.com/app/board/BOARD_ID/)
MIRO_BOARD_ID=your_board_id_here

# Miro API Base URL
MIRO_API_BASE_URL=https://api.miro.com/v2
